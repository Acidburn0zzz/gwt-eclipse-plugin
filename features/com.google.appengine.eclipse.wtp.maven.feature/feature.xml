<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.google.appengine.eclipse.wtp.maven.feature"
      label="%featureName"
      version="1.0.0.qualifier"
      provider-name="%providerName">

   <description>
      Support for importing a Maven project for Google App Engine as a WTP project.
   </description>

   <copyright>
      Copyright 2014
   </copyright>

   <license>
<![CDATA[
      @FEATURE_LICENSE@
]]>
   </license>

   <url>
      <update label="Google Update Site for Eclipse" url="@FEATURE_UPDATE_SITE_URL@" />
   </url>

   <requires>
      <import plugin="com.google.appengine.eclipse.core"/>
      <import plugin="com.google.appengine.eclipse.wtp"/>
      <import plugin="com.google.appengine.eclipse.wtp.jpa"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.equinox.common"/>
      <import plugin="org.eclipse.m2e.core"/>
      <import plugin="org.eclipse.m2e.core.ui"/>
      <import plugin="org.eclipse.m2e.discovery"/>
      <import plugin="org.eclipse.m2e.jdt"/>
      <import plugin="org.eclipse.m2e.launching"/>
      <import plugin="org.eclipse.m2e.maven.runtime" version="1.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.m2e.wtp"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.jdt.ui"/>
      <import plugin="org.eclipse.jpt.jpa.core"/>
      <import plugin="org.eclipse.jst.common.project.facet.core"/>
      <import plugin="org.eclipse.jst.common.project.facet.ui"/>
      <import plugin="org.eclipse.jst.j2ee"/>
      <import plugin="org.eclipse.jst.server.core"/>
      <import plugin="org.eclipse.wst.common.frameworks.ui"/>
      <import plugin="org.eclipse.wst.common.project.facet.core"/>
      <import plugin="org.eclipse.wst.common.project.facet.ui"/>
      <import plugin="org.eclipse.wst.server.core"/>
      <import plugin="org.eclipse.wst.server.ui"/>
   </requires>

   <plugin
         id="com.google.appengine.eclipse.wtp.maven"
         download-size="0"
         install-size="0"
         version="0.0.0.qualifier"
         unpack="false"/>

</feature>
